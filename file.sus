func factorial (int n): int {
    int i;
    int output = 1;

    n = n + 1;
    loop (for i in range(n)) {
        if (i == 0) {
            i = 1;
        }
        output = output * i;
    }
    return output;
}

main () {
    af runFactorial() => {
        int r = factorial(5);
        print(r);
    };
    runFactorial();
}
